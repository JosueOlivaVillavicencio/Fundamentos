#include <iostream>

using namespace std;

int main()
{
    int i,N;
    cout<<"ingrese el numero de veces a repetir: ";
    cin>>N;
    cout<<"salida del mientras"<<endl;
    i=0;
    while(i<N)
    {
        i++;
        cout<<i<<endl;
        
    }
    cout<<"salida de para"<<endl;
    for(int i=0;i<=10;i++)
         cout<<i<<endl;
         cout<<"salida del repetir"<<endl;
         i=0;
         do{
             
         } while(i<10);
         
         
    return 0;
}
